#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jakubmurin/Documents/metrify/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/bin/node_modules:/Users/jakubmurin/Documents/metrify/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/node_modules:/Users/jakubmurin/Documents/metrify/node_modules/.pnpm/eslint@7.32.0/node_modules:/Users/jakubmurin/Documents/metrify/node_modules/.pnpm/node_modules:/Users/jakubmurin/Documents/metrify/node_modules:/Users/jakubmurin/Documents/node_modules:/Users/jakubmurin/node_modules:/Users/node_modules:/node_modules:/Users/jakubmurin/Documents/metrify/node_modules/.pnpm/eslint-config-standard@16.0.3_dfa64ed3583b1f068d7b5761cc0291ce/node_modules/eslint-config-standard/node_modules:/Users/jakubmurin/Documents/metrify/node_modules/.pnpm/eslint-config-standard@16.0.3_dfa64ed3583b1f068d7b5761cc0291ce/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/jakubmurin/Documents/metrify/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/bin/node_modules:/Users/jakubmurin/Documents/metrify/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/node_modules:/Users/jakubmurin/Documents/metrify/node_modules/.pnpm/eslint@7.32.0/node_modules:/Users/jakubmurin/Documents/metrify/node_modules/.pnpm/node_modules:/Users/jakubmurin/Documents/metrify/node_modules:/Users/jakubmurin/Documents/node_modules:/Users/jakubmurin/node_modules:/Users/node_modules:/node_modules:/Users/jakubmurin/Documents/metrify/node_modules/.pnpm/eslint-config-standard@16.0.3_dfa64ed3583b1f068d7b5761cc0291ce/node_modules/eslint-config-standard/node_modules:/Users/jakubmurin/Documents/metrify/node_modules/.pnpm/eslint-config-standard@16.0.3_dfa64ed3583b1f068d7b5761cc0291ce/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../eslint@7.32.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../eslint@7.32.0/node_modules/eslint/bin/eslint.js" "$@"
fi
